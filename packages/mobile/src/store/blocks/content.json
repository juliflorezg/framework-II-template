    "store.signin.with-email-and-password": {
        "children": [
            {
                "componentName": "store-form",
                "props": {
                    "context": "useStartAuthentication",
                    "mode": "onChange",
                    "parseInput": {
                        "to": {
                            "password": "password"
                        }
                    },
                    "schemaValidation": [
                        {
                            "id": "password",
                            "type": "text",
                            "validationType": "string",
                            "validations": [
                                {
                                    "type": "min",
                                    "params": [
                                        8,
                                        "La contraseña no puede ser menor a 8 caracteres"
                                    ]
                                },
                                {
                                    "type": "max",
                                    "params": [
                                        50,
                                        "La contraseña no puede ser mayor a 50 caracteres"
                                    ]
                                },
                                {
                                    "type": "matches",
                                    "params": [
                                        "/^.*[0-9].*$/",
                                        "Debe contener almenos un numero"
                                    ]
                                },
                                {
                                    "type": "matches",
                                    "params": [
                                        "/^.*[a-z].*$/",
                                        "Debe contener almenos una letra minuscula"
                                    ]
                                },
                                {
                                    "type": "matches",
                                    "params": [
                                        "/^.*[A-Z].*$/",
                                        "Debe contener almenos una letra mayuscula"
                                    ]
                                },
                                {
                                    "type": "required",
                                    "params": [
                                        "La contraseña es requerida"
                                    ]
                                }
                            ]
                        }
                    ],
                    "style": "signin-email-wrapper"
                },
                "children": [
                    {
                        "componentName": "rich-text",
                        "props": {
                            "text": "Solo falta tu clave",
                            "style": "signin-title"
                        }
                    },
                    {
                        "componentName": "rich-text",
                        "props": {
                            "text": "Escribe tu contraseña para entrar",
                            "style": "signin-description"
                        }
                    },
                    {
                        "componentName": "store-form.input",
                        "props": {
                            "name": "password",
                            "placeholder": "Contraseña",
                            "style": "form-input-default"
                        }
                    },
                    {
                        "componentName": "store-form.submit",
                        "props": {
                            "buttonText": "Continuar",
                            "style": "submit-button"
                        }
                    }
                ]
            }
        ]
    },
